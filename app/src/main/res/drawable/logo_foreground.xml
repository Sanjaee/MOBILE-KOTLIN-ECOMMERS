<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- Logo zacode sebagai foreground untuk adaptive icon -->
    <!-- Menggunakan inset dalam dp untuk safe zone -->
    <item
        android:left="18dp"
        android:top="18dp"
        android:right="18dp"
        android:bottom="18dp">
        <bitmap
            android:gravity="fill"
            android:src="@drawable/logo" />
    </item>
</layer-list>
